{
    "name": "thingsdb-lang",
    "displayName": "ThingsDB Lang",
    "description": "Syntax highlighting for ThingsDB",
    "version": "0.0.6",
    "publisher": "transceptor-technology",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "thingsdb",
                "aliases": [
                    "ThingsDB",
                    "thingsdb"
                ],
                "extensions": [
                    ".ti"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "thingsdb",
                "scopeName": "source.ti",
                "path": "./syntaxes/thingsdb.tmLanguage.json"
            }
        ]
    },
    "license": "SEE LICENSE IN LICENSE FILE",
    "bugs": {
        "url": "https://github.com/thingsdb/vscode-thingsdb-lang/issues",
        "email": "jeroen@transceptor.technology"
    },
    "icon": "logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/thingsdb/vscode-thingsdb-lang.git"
    }
}
