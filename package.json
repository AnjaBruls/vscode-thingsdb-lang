{
    "name": "thingsdb-lang",
    "displayName": "ThingsDB Lang",
    "description": "Syntax highlighting for ThingsDB",
    "version": "0.0.5",
    "publisher": "transceptor-technology",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "thingsdb",
                "aliases": [
                    "ThingsDB",
                    "thingsdb"
                ],
                "extensions": [
                    ".ti"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "thingsdb",
                "scopeName": "source.ti",
                "path": "./syntaxes/thingsdb.tmLanguage.json"
            }
        ]
    },
    "icon": "https://github.com/thingsdb/vscode-thingsdb-lang/blob/master/logo.png"
}
